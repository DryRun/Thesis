@article{Martin:2009it,
author = {Martin, Stephen P},
title = {{Extra vector-like matter and the lightest Higgs scalar boson mass in low-energy supersymmetry}},
journal = {arXiv.org},
year = {2009},
eprint = {0910.2732},
